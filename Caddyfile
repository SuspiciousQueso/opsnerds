jobs.mspguild.tech {
    # Point to the 'php' service in docker-compose
    # Assumes your PHP container is running a webserver on port 80 
    # (or use php-fpm if your Dockerfile is set up for that)
    reverse_proxy php:80
    
    # Enable compression
    encode zstd gzip
    
    # Log requests
    log {
        output file /data/access.log
    }
}